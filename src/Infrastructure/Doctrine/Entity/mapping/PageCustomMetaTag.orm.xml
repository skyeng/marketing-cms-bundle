<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Skyeng\MarketingCmsBundle\Domain\Entity\PageCustomMetaTag" table="cms_page_custom_meta_tag"
            repository-class="Skyeng\MarketingCmsBundle\Infrastructure\Doctrine\Repository\PageOpenGraphDataRepository">
        <id name="id" type="cms_id"/>
        <many-to-one field="page" target-entity="Skyeng\MarketingCmsBundle\Domain\Entity\Page">
            <cascade>
                <cascade-persist/>
            </cascade>
        </many-to-one>
        <field name="name" nullable="true"/>
        <field name="property" nullable="true"/>
        <field name="content" type="text"/>
    </entity>

</doctrine-mapping>
